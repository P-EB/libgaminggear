#!/bin/sh

set -e

purge_libgaminggear () {
    groupdel uinput-users
}

case "$1" in
    purge)
        purge_libgaminggear
    ;;
    remove|upgrade|deconfigure)
    ;;

    failed-upgrade)
    ;;

    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0
