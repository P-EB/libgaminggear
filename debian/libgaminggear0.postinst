#!/bin/sh

set -e

# Extracted from INSTALL file
configure_libgaminggears (){
    touch --no-create /usr/share/icons/hicolor
}

case "$1" in
    configure)
        configure_libgaminggears
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0
